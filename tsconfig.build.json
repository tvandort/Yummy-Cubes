{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,

    "lib": ["ES2017", "DOM", "ES2019"],
    "downlevelIteration": true,

    "noImplicitAny": false,
    "strict": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": ["node_modules", "out"]
}
